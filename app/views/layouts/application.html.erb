                <li>
                  <a href="#"><i class="icon_chat_alt"></i> Chats</a>
                </li>
                <li>
                  <a href="<%= destroy_user_session_path %>" data-method="DELETE" data-confirm="Are you sure?"><i class="icon_key_alt"></i> Log Out</a>
                </li>
                <li>
                  <a href="documentation.html"><i class="icon_key_alt"></i> Documentation</a>
                </li>
                <li>
                  <a href="documentation.html"><i class="icon_key_alt"></i> Documentation</a>
                </li>
              </ul>
            </li>
            <!-- user login dropdown end -->
          </ul>
